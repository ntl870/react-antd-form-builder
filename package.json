    "antd": "^5.16.4",
    "classnames": "^2.5.1",
    "react": "^18.2.0",
    "react-signature-canvas": "^1.0.6",
    "tailwind-merge": "^2.3.0",
    "use-immer": "^0.9.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.79",
    "@types/react-signature-canvas": "^1.0.5",
